<UI xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/">
	<Script file="spell.lua" /> 

	<Frame name="CoolLevelUp_SpellTemplate" virtual="true">
		<Size>
			<AbsDimension x="384" y="16"/>
		</Size>
		<Scripts>
			<OnLoad>
				CoolLevelUp.Spell.OnLoad(self);
			</OnLoad>
			<OnUpdate>
				CoolLevelUp.Spell.OnUpdate(self, elapsed);
			</OnUpdate>
		</Scripts>
	</Frame>

	<FontString name="CoolLevelUp_SpellTextTemplate" inherits="NumberFontNormal" virtual="true">
		<Size>
			<AbsDimension x="320" y="16"/>
		</Size>
	</FontString>
</UI>