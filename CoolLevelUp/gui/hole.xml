<UI xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/">
	<Script file="hole.lua" /> 

	<Frame name="CoolLevelUp_HoleTemplate" virtual="true">
		<Size>
			<AbsDimension x="96" y="8"/>
		</Size>
		<Scripts>
			<OnLoad>
				CoolLevelUp.Hole.OnLoad(self);
			</OnLoad>
			<OnUpdate>
				CoolLevelUp.Hole.OnUpdate(self, elapsed);
			</OnUpdate>
		</Scripts>
	</Frame>

	<FontString name="CoolLevelUp_NameTextTemplate" inherits="GameFontNormalSmall" virtual="true">
		<Size>
			<AbsDimension x="48" y="16"/>
		</Size>
	</FontString>
	<FontString name="CoolLevelUp_StatTextTemplate" inherits="NumberFontNormal" virtual="true">
		<Size>
			<AbsDimension x="48" y="16"/>
		</Size>
	</FontString>
</UI>